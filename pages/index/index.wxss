.company_name {
  border: 1rpx solid #e0e0e0;
  border-radius: 10rpx;
  box-shadow: 0 0 5rpx #e0e0e0;
  padding: 10rpx;
  font-size: 28rpx;
}

.account_object {
  margin-left: 30rpx;
  border: 1rpx solid #e0e0e0;
  border-radius: 10rpx;
  box-shadow: 0 0 5rpx #e0e0e0;
  padding: 10rpx;
  font-size: 28rpx;
}
.account_object_id_even{
  background-color: #adfd95;
}
.account_object_id_uneven{
  background-color: #3a82e3;
}

